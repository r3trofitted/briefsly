<header class="navbar">
  <nav class="container navbar__inner">
    <%= link_to schedule_path, class: "title-logo" do %>
      <%= image_tag("title.png", size: "139x47") %>
    <% end %>

    <ul class="navbar__links">
      <li>
        <%= link_to_unless_current "Open a slot", new_slot_path, class: "navbar__link" %>
      </li>
      <li>
        <%= link_to_unless_current "Look for a jam", slots_path, class: "navbar__link" %>
      </li>
      <li>
        <%= link_to_unless_current "Schedule", schedule_path, class: "navbar__link" %>
      </li>
      <li>
        <%= link_to_unless_current "Profile", edit_profile_path, class: "navbar__link" %>
      </li>
      <li>
        <%= link_to "Sign out", session_path, data: { turbo_method: :delete }, class: "button" %>
      </li>
    </ul>
  </nav>
</header>
